  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: TestPlan",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/tests/testplan/test_plan.py",
      "console": "integratedTerminal",
      "python": "${workspaceFolder}/.testplan/bin/python",
      "args": []
    },

    {
      "name": "Python: FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "cwd": "${workspaceFolder}/aladdin/app",
      "python": "${workspaceFolder}/.fastapi/bin/python",
      "args": ["main:app"],
      "jinja": true
    },
    {
      "name": "Python: Prefect Flows",
      "type": "python",
      "request": "launch",
      "module": "aladdin.engine.prefect_example",
      "python": "${workspaceFolder}/.prefect/bin/python"
    }
  ]
}
